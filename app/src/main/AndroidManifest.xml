<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.miexpense.expense.miexpense" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExpenseSubmission"
            android:label="@string/title_activity_expense_submission" >
        </activity>
        <activity
            android:name=".ExpenseDetail"
            android:label="@string/title_activity_expense_detail" >
        </activity>
        <provider
            android:authorities="com.miexpense.expense.miexpense.ExpenseProvider"
            android:name=".ExpenseProvider"/>
    </application>

</manifest>
